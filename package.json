{
  "name": "cypress-test-demo",
  "version": "1.0.0",
  "description": "Cypress Test Demo",
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cy:open": "cypress open",
    "cy:run": "cypress run --spec 'cypress/integration/examples/*'",
    "cy:e2e": "start-server-and-test serve http-get://localhost:8080 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kaxiu008/cypress-test-demo.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "cypress": "3.1.0",
    "start-server-and-test": "1.7.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
